{
  "name": "nxus-router",
  "version": "3.0.0-1",
  "description": "An express-based router for Nxus",
  "main": "lib",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register -R spec",
    "compile": "babel src --out-dir lib ",
    "prepublish": "npm run compile",
    "postpublish": "npm run docs-readme && npm run commit-update-docs",
    "docs-readme": "export NAME=`npm view . name`; export VERSION=`npm view . version`; documentation readme ./src/*.js --name $NAME --project-version $VERSION --readme-file ./README.md -s $NAME",
    "commit-update-docs": "git add ./README.md && git commit -m 'Updated README API Docs' && git push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nxus/router-express.git"
  },
  "author": "info@nxusapp.com",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.0.5",
    "body-parser": "^1.10.1",
    "compression": "^1.4.3",
    "connect-flash": "^0.1.1",
    "express": "^4.10.7",
    "underscore": "^1.7.0",
    "express-session": "^1.12.1",
    "session-file-store": "0.2.0"
  },
  "bugs": {
    "url": "https://github.com/nxus/router-express/issues"
  },
  "homepage": "https://github.com/nxus/router-express",
  "peerDependencies": {
    "nxus-core": "^3.0.0-0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "mocha": "^2.2.5",
    "should": "^7.0.2",
    "sinon": "^1.17.2",
    "chai": "^3.5.0",
    "documentation": "^4.0.0-beta",
    "chai-as-promised": "^5.2.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0"
  }
}
